<h1>Welcome, Admin <%= current_user.email%>!</h1>
<span>search feature</span>
<span>sort feature</span>
<table>
  <thread>
    <tr>
      <th>Transaction ID</th>
      <th>User ID</th>
      <th>Symbol</th>
      <th>Company Name</th>
      <th>Unit Price</th>
      <th>Quantity</th>
      <th>Amount</th>
      <th>Type</th>
      <th>Transacted At</th>
    </tr>
  </thread>
  <tbody>
    <% @transactions.each do |t| %>
      <tr>
        <td><%=t.id%></td>
        <td><%=t.user_id%></td>
        <td><%=t.symbol%></td>
        <td><%= t.company_name%></td>
        <td><%= t.unit_price%></td>
        <td><%= t.quantity%></td>
        <td><%= t.amount%></td>
        <td><%= t.transaction_type%></td>
        <td><%= t.created_at%></td>
      </tr>
    <%end%>
  </tbody>
</table>
